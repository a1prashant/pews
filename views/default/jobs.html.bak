{{extend 'layout.html'}}
<h1>Jobs</h1>

<!--
{{=rows}}
-->

<table class="table">
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>For Freshers?</th>
        <th>Date Created</th>
    </tr>
    {{for row in rows:}}
    <tr>
        <td><a href="{{=URL('show', args=row.id)}}">{{=row.id}}</a></td>
        <td><a href="{{=URL('show', args=row.title)}}">{{=row.title}}</a></td>
        <td>{{=row.created_on}}</td>
    </tr>
    {{pass}}
</table>
